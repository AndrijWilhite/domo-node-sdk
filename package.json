{
  "name": "domo-sdk",
  "version": "0.1.0",
  "description": "Javascript Domo SDK",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src",
    "build": "rm -rf ./dist && NODE_ENV=production npm run lint && BABEL_ENV=production babel src -d dist",
    "test": "npm run lint && NODE_ENV=test ./node_modules/.bin/mocha --opts .mocharc",
    "test:watch": "npm run test -- --watch",
    "coverage": "NODE_ENV=test istanbul cover _mocha -- --opts .mocharc",
    "postbuild": "cp package.json ./dist && cp README.md ./dist && cp domo.png ./dist",
    "upload": "npm run test && npm version && npm run build && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/domoinc/domo-node-sdk"
  },
  "keywords": [
    "Domo"
  ],
  "license": "MIT",
  "dependencies": {
    "debug": "^2.6.6",
    "request": "^2.81.0",
    "request-promise": "^4.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-preset-babili": "0.0.12",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^3.5.0",
    "eslint": "^2.8.0",
    "eslint-config-airbnb": "^8.0.0",
    "eslint-plugin-import": "^1.6.1",
    "istanbul": "^0.4.5",
    "mocha": "^3.3.0",
    "nock": "^9.0.13",
    "nodemon": "^1.11.0",
    "sinon": "^2.2.0"
  }
}
